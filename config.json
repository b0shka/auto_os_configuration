{
	"config": {
		"env": ".env",
		"theme": {
			"path": "themes/WhiteSur-Dark.tar.xz",
			"name": "WhiteSur-Dark"
		},
		"icons": {
			"path": "icons/kora.tar.xz",
			"name": "kora"
		},
		"ssh": {
			"git": ".ssh/git",
			"git_pub": ".ssh/git.pub"
		},
		"alacritty": "config_apps/alacritty.yml",
		"vscode": "config_apps/settings.json",
		"keepassxc": "config_apps/keepassxc.ini"
	},
	"apps": {
		"delete": ["gnome-contacts", "gnome-calendar", "baobab", "info", "seahorse", "totem", "yelp", "gucharmap", "gnome-font-viewer", "gnome-weather", "com.github.donadigo.eddy", "gnome-power-manager", "popsicle-gtk", "libgl1-mesa-dev", "firefox", "geary"],
		"install": ["ssh", "keepassxc", "git", "transmission", "gnome-tweaks", "build-essential", "chrome-gnome-shell", "mat2", "alacritty", "filezilla", "python3-pip", "python3-venv", "flatpak", "code", "vlc", "npm"],
		"install_flatpak": ["org.telegram.desktop", "md.obsidian.Obsidian", "com.wps.Office", "com.getpostman.Postman", "com.mongodb.Compass", "com.brave.Browser", "ru.yandex.Browser", "com.todoist.Todoist"],
		"favorite": ["com.brave.Browser.desktop", "ru.yandex.Browser.desktop", "org.gnome.Nautilus.desktop", "com.alacritty.Alacritty.desktop", "org.telegram.desktop.desktop", "com.todoist.Todoist.desktop", "md.obsidian.Obsidian.desktop", "code.desktop", "com.getpostman.Postman.desktop", "org.keepassxc.KeePassXC.desktop", "org.gnome.gedit.desktop", "gnome-system-monitor.desktop", "io.elementary.appcenter.desktop", "pop-cosmic-applications.desktop"],
		"default": {
			"default-web-browser": "com.brave.Browser.desktop"
		},
		"versions_backend_tools": {
			"golang": "1.21.1",
			"migrate": "v4.16.2",
			"golangci-lint": "v1.54.2"
		}
	},
	"paths": {
		"notes": "personal/Заметки",
		"venv": "resources/env"
	},
	"aliases": {
		"backup": "~/programming/scripts/backup/backup.sh",
		"venv": "source ~/resources/env/bin/activate"
	},
	"os": {
		"hotkeys": {
			"close": "['<Super>x']",
			"minimize": "['<Super>z']"
		},
		"dock": {
			"dash-max-icon-size": 45,
			"background-opacity": 0.8,
			"show-trash": true,
			"show-mounts": false,
			"dock-fixed": false,
			"intellihide": true
		},
		"pop_cosmic": {
			"show-application-menu": true,
			"show-applications-button": false,
			"show-workspaces-button": false
		},
		"interface": {
			"clock-show-date": true,
			"enable-hot-corners": false,
			"show-battery-percentage": true,
			"enable-animations": true,
			"clock-format": "24h"
		},
		"night_light": {
			"night-light-enabled": true,
			"night-light-schedule-automatic": false,
			"night-light-schedule-from": 21.0,
			"night-light-schedule-to": 8.0
		},
		"screensaver": {
			"lock-enabled": false,
			"lock-delay": 1500,
			"ubuntu-lock-on-suspend": false
		},
		"power": {
			"power-button-action": "nothing",
			"sleep-inactive-ac-type": "nothing",
			"sleep-inactive-battery-type": "nothing"
		},
		"privacy": {
			"remember-recent-files": false,
			"remove-old-trash-files": false,
			"remove-old-temp-files": false
		},
		"button_layout": ":minimize,close",
		"volume_above": true,
		"first_day_week": 1,
		"automatic_timezone": true,
		"timezone": "Asia/Yekaterinburg",
		"keyboard_layout": "[('xkb', 'us'), ('xkb', 'ru')]",
		"screen_shutdown_delay": 0
	},
	"mega": {
		"link_download": "https://mega.nz/linux/repo/xUbuntu_22.04/amd64/megacmd-xUbuntu_22.04_amd64.deb",
		"name_file": "megacmd-xUbuntu_22.04_amd64.deb",
		"backup_name": "backup.tar.xz",
		"download_folders": ["inbox", "personal", "programming", "study", "resources"],
		"exclude_folders": ["node_modules", ".git", ".vscode", ".idea", ".next", "__pycache__"]
	},
	"vscode": {
		"extensions": ["Codeium.codeium", "dsznajder.es7-react-js-snippets", "golang.go", "Gruntfuggly.todo-tree", "MS-CEINTL.vscode-language-pack-ru", "TabNine.tabnine-vscode", "vscode-icons-team.vscode-icons"]
	},
	"nautilus": {
		"preferences": {
			"default-folder-viewer": "list-view",
			"show-hidden-files": false,
			"click-policy": "double"
		},
		"list_view": {
			"default-zoom-level": "small",
			"default-visible-columns": "['name', 'size']"
		},
		"compression_format": "tar.xz"
	},
	"gedit": {
		"editor": {
			"display-line-numbers": false,
			"display-right-margin": false,
			"display-overview-map": false,
			"bracket-matching": true,
			"highlight-current-line": false,
			"wrap-mode": "word",
			"tabs-size": 4,
			"insert-spaces": false,
			"auto-indent": true,
			"create-backup-copy": false,
			"auto-save": true,
			"auto-save-interval": 1,
			"use-default-font": true,
			"scheme": "tango"
		},
		"ui": {
			"statusbar-visible": false
		}
	},
	"remove_files": {
		"config": [".env", "themes", "icons", "config_apps", "config.json"],
		"extra": ["Видео", "Документы", "Изображения", "Музыка", "Общедоступные", "Шаблоны"]
	}
}
